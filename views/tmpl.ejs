<!doctype html>
<!--[if lt IE 7]><html lang="en" class="no-js ie6"><![endif]-->
<!--[if IE 7]><html lang="en" class="no-js ie7"><![endif]-->
<!--[if IE 8]><html lang="en" class="no-js ie8"><![endif]-->
<!--[if gt IE 8]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->

<head>
  <meta charset="UTF-8">
  <title>Distribution Page Template</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <link rel="shortcut icon" href="../favicon.png">
  <link rel="stylesheet" href="../assets/css/bootstrap.min.css">
  <link rel="stylesheet" href="../assets/css/animate.css">
  <link rel="stylesheet" href="../assets/css/font-awesome.min.css">
  <link rel="stylesheet" href="../assets/css/slick.css">
  <link rel="stylesheet" href="../assets/js/rs-plugin/css/settings.css">
  <link rel="stylesheet" href="../assets/css/styles.css">
  <script type="text/javascript" src="../assets/js/modernizr.custom.32033.js"></script>
  <!--[if lt IE 9]>
<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
<![endif]-->
</head>

<body>
  <header>
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation" style="color: <%= textColor %> !important;background: #<%= background %>;background: -moz-linear-gradient(-45deg, #<%= background %> 0%, #336799 100%);background: -webkit-gradient(linear, left top, right bottom, color-stop(0%, #<%= background %>), color-stop(100%, #336799));background: -webkit-linear-gradient(-45deg, #<%= background %> 0%, #<%= background %> 100%);background: -o-linear-gradient(-45deg, #<%= background %> 0%, #336799 100%);background: -ms-linear-gradient(-45deg, #<%= background %> 0%, #336799 100%);background: linear-gradient(-45deg, #<%= background %> 0%, #336799 100%);filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#<%= background %>, endColorstr=#<%= background %>, GradientType=1);">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
            <span class="fa fa-bars fa-lg"></span>
          </button>
          <a class="navbar-brand" href="index.html">
            <p style="font-size: 40px; font-weight: 300; color:rgba(255,255,255,1); padding-top: 15px;">
              <%= company_name_header %>
            </p>
          </a>
        </div>
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
          <ul class="nav navbar-nav navbar-right">
            <li><a href="#about">about</a>
            </li>
            <li><a href="#screens">pictures</a>
            </li>
            <li><a href="#demo">demo</a>
            </li>
            <li><a class="getApp" href="#getApp">get app</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
  </header>
  <div class="wrapper" style="padding-top: 90px;">
    <section id="about">
      <div class="container">
        <div class="section-heading scrollpoint sp-effect3">
          <h1>About</h1>
          <div class="divider"></div>
          <p>
            <%= company_name_subheader %>
          </p>
        </div>
        <div class="row">
          <% for (var x=0; x<=features.length-1; x++)  { %>
            <div class="col-md-3 col-sm-3 col-xs-6">
              <div class="about-item scrollpoint sp-effect2">
                <i class='fa <%= features[x].feature_icon %> fa-2x'></i>
                <h3>
                  <%= features[x].feature_title %>
                </h3>
                <p>
                  <%= features[x].feature_text %>
                </p>
              </div>
            </div>
            <% } %>
        </div>
      </div>
    </section>
    <section id="screens">
      <div class="container">
        <div class="section-heading sp-effect3">
          <h1>Pictures</h1>
          <div class="divider"></div>
          <p>Some companies images....</p>
        </div>
        <div class="slider filtering sp-effect5">
          <% for (var x=0; x<=pictures.length-1;x++) { %>
            <div class="one">
              <img src="<%= pictures[x] %>" style="width: 100%;" alt="" />
            </div>
            <% } %>
        </div>
    </section>
    <section id="demo">
      <div class="container">
        <div class="section-heading scrollpoint sp-effect3">
          <h1>Demo</h1>
          <div class="divider"></div>
          <p>Take a closer look in more detail</p>
        </div>
        <div class="row">
          <div class="col-md-8 col-md-offset-2 scrollpoint sp-effect2">
            <div class="video-container">
              <iframe src="<%= video %>"></iframe>
            </div>
          </div>
        </div>
      </div>
    </section>
    <section id="getApp">
      <div class="container-fluid">
        <div class="section-heading inverse scrollpoint sp-effect3">
          <h1>Get App</h1>
          <div class="divider"></div>
          <p>Choose your native platform and get started!</p>
        </div>
        <div class="row">
          <div class="col-md-12">
            <div class="hanging-phone scrollpoint sp-effect2 hidden-xs">
              <img src="../assets/img/freeze/freeze-angled2.png" alt="">
            </div>
            <div class="platforms">
              <% for (var x=0; x<=urls.length-1; x++) { %>
                <a href="<%= urls[x].url %>" class="btn btn-primary inverse scrollpoint sp-effect1">
                <i class="fa <%= urls[x].icon %> fa-3x pull-left"></i>
                  <span>Download for</span><br>
                    <b><%= urls[x].platform %></b>
                </a>
                <% } %>
            </div>
          </div>
        </div>
      </div>
    </section>
    </div>
    <script src="../assets/js/jquery-1.11.1.min.js"></script>
    <script src="../assets/js/bootstrap.min.js"></script>
    <script src="../assets/js/slick.min.js"></script>
    <script src="../assets/js/placeholdem.min.js"></script>
    <script src="../assets/js/rs-plugin/js/jquery.themepunch.plugins.min.js"></script>
    <script src="../assets/js/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
    <script src="../assets/js/waypoints.min.js"></script>
    <script src="../assets/js/scripts.js"></script>
    <script>
      $(document).ready(function () {
        appMaster.preLoader();
      });
    </script>
</body>

</html>';